#: scene301-help/mhtropi1.txt:4
#, no-wrap
msgid "Procedure"
msgstr ""

#. type: Plain text
#: scene301-help/mhtropi1.txt:6
#, no-wrap
msgid "The location of the explosive is marked by a red cross on your mini map."
msgstr ""

