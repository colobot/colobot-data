#, no-wrap
msgid "Uranium"
msgstr "Urânio"

#. type: Resume-text
#: ../scene.txt:2
#, no-wrap
msgid "An attempt to retrieve some uranium ore."
msgstr "Uma tentativa de recuperar algum minério de urânio."

#. type: \b; header
#: ../help/help.E.txt:1
#, no-wrap
msgid "From Mission Control"
msgstr "Do Centro de Controle de Missões"

#. type: Plain text
#: ../help/help.E.txt:4
#, no-wrap
