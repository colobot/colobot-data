"\\tab;Atmosphere:       CO2, oxygen, nitrogen\n"
"\\tab;Wind:             2.3 mps\n"
"\\tab;Titanium ore:     none\n"
"\\tab;Uranium ore:      none"
msgstr ""
"\\tab;Temperatura:      44,3C\n"
"\\tab;Atmosfera:        CO2, tlen, azot\n"
"\\tab;Wiatr:            2,3 m/s\n"
