#, no-wrap
msgid ""
"\\tab;Energy source:    none\n"
"\\tab;Titanium ore:     none\n"
"\\tab;Uranium ore:      none"
msgstr ""
"\\tab;Energie:          aucune\n"
"\\tab;Minerai de titane:aucun\n"
"\\tab;Minerai d'uranium:aucun"

#. type: Source code
#: ../help/report.E.txt:16
#, no-wrap
