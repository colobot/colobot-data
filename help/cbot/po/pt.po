msgstr "<a cbot|category>Categoria</a> do objeto."

#. type: \t; header
#: ../E/produce.txt:18
#, no-wrap
msgid "program: <code><a cbot|string>string</a></code>"
msgstr "programa: <code><a cbot|string>string</a></code>"

#. type: Plain text
#: ../E/produce.txt:22
