"\\tab;Атмосфера:       CO2, SO2, HCN, HS, CH4, NH3\n"
"\\tab;Ветер:             2.2 м/с\n"
"\\tab;Титановая руда:     везде\n"
"\\tab;Урановая руда:      немного"

#. type: Source code
#: ../help/report.E.txt:11
#, no-wrap
msgid "-> SUBSOIL<c/>"
msgstr "-> ПОДПОЧВА<c/>"

