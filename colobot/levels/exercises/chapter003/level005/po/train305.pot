#: ../help/help.E.txt:35
#, no-wrap
msgid "See also"
msgstr ""

#. type: Plain text
#: ../help/help.E.txt:36
#, no-wrap
msgid "<a cbot>Programming</a>, <a cbot|type>types</a> and <a cbot|category>categories</a>."
msgstr ""

