#. type: Source code
#: ../E/object.txt:63
#, no-wrap
msgid "<code>velocity</code>"
msgstr ""

#. type: Plain text
#: ../E/object.txt:64
#, no-wrap
msgid "Current velocity of the object. Should be treated as a three-dimensional vector."
msgstr ""

